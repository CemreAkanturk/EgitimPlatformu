
@{
    ViewBag.Title = "DersBilgiler";
    Layout = "~/Views/Shared/firmaLayout.cshtml";


}


    <div class="page-header">
        <div class="row align-items-end">
            <div class="col-lg-8">
                <div class="page-header-title">
                    <i class="ik ik-file-text bg-blue"></i>
                    <div class="d-inline">
                        <h2>Matematik</h2>
                    </div>
                </div>
            </div>
     
        </div>
    </div>

    <div class="row">
        <div class="col-lg-4 col-md-5">
            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        <img src="data:image/jpeg;base64," class="rounded-circle" width="150" />
                        <a data-toggle="modal" href="#modelim" id="#myBtn"><h4 class="card-title mt-10">cemre akanturk</h4></a>
                        <p class="card-subtitle">Matematik Bilimci</p>
                        <div class="row text-center justify-content-md-center">
                            <div class="col-4"><i class="ik ik-image"></i> <font class="font-medium">2 Eğitim</font></a></div>
                        </div>
                    </div>
                </div>
                <hr class="mb-0">
            </div>
        </div>
        <div class="col-lg-8 col-md-7">
            <div class="card">
                <ul class="nav nav-pills custom-pills" id="pills-tab" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" id="pills-profile-tab" data-toggle="pill" href="#last-month" role="tab" aria-controls="pills-profile" aria-selected="false">Ders Bilgileri</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="pills-setting-tab" data-toggle="pill" href="#previous-month" role="tab" aria-controls="pills-setting" aria-selected="false">Katılımcılar</a>
                    </li>
                </ul>
                <div class="tab-content" id="pills-tabContent">
                    <div class="tab-pane fade show active" id="last-month" role="tabpanel" aria-labelledby="pills-profile-tab">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-3 col-6">
                                    <strong>Ders Adı</strong>
                                    <br>
                                    <p class="text-muted">Matematik</p>
                                </div>
                                <div class="col-md-3 col-6">
                                    <strong>Eğitim Seans Kodu</strong>
                                    <br>
                                    <p class="text-muted">12</p>
                                </div>
                                <div class="col-md-3 col-6">
                                    <strong>Ders Kategorisi</strong>
                                    <br>
                                    <p class="text-muted">Matematik</p>
                                </div>

                            </div>
                            <hr>
                            <strong>Kazanımlar</strong>

                            <p class="mt-30">Kazanımlar.</p>
                            <h4 class="mt-30">Eğitim İçerik</h4>
                            <hr>
                            <div class="col-lg-12 col-md-6 col-sm-12">
                                <div class="widget bg-primary">
                                    <div class="widget-body">
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="state">
                                                <h6>Video</h6>
                                                <h2>1</h2>
                                            </div>
                                            <div class="icon">
                                                <i class="ik ik-film"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="widget bg-success">
                                    <div class="widget-body">
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="state">
                                                <h6> Soru</h6>
                                                <h2>5</h2>
                                            </div>
                                            <div class="icon">
                                                <i class="ik ik-edit-1"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="widget bg-warning">
                                    <div class="widget-body">
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="state">
                                                <h6> Sınıf İçi Etkinlik</h6>
                                                <h2>2</h2>
                                            </div>
                                            <div class="icon">
                                                <i class="ik ik-award"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="previous-month" role="tabpanel" aria-labelledby="pills-setting-tab">
                        <div class="col-sm-12">
                            <div class="card>
                                <div class="card-body">
                                    <div class="dt-responsive">
                                        <table id="simpletable"
                                               class="table table-striped table-bordered nowrap">
                                            <thead>
                                                <tr>
                                                    <th>Ad Soyad</th>
                                                    <th>Bölüm</th>
                                                    <th>Yaş</th>
                                                    <th>Cinsiyet</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                
                                                <tr>
                                                    <td>Doruk Özgür</td>
                                                    <td>ArGe</td>
                                                    <td>22</td>
                                                    <td>Erkek</td>
                                                </tr>
                                                <tr>
                                                    <td>Oktay Pekmek</td>
                                                    <td>ArGe</td>
                                                    <td>31</td>
                                                    <td>Erkek</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>



</div>

@section Styles{
    <link href="~/Content/plugins/datatables.net-bs4/css/dataTables.bootstrap4.min.css" rel="stylesheet" />

}



@section Scripts
{
    <script src="~/Content/plugins/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/plugins/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>
    @*@<script src="~/Content/js/datatables.js"></script>*@
    <script type="text/javascript">
        $(document).ready(function () {
            $('simpletable').DataTable();
        });


    </script>
    <script>
        $(document).ready(function () {
            $("#myBtn").click(function () {
                $("#myModal").modal();
            });
        });
    </script>

}
