
@{
    ViewBag.Title = "CalisanDersler";
    Layout = "~/Views/Shared/calisanLayout.cshtml";
}
<div class="row">
    <div class="col-xl-4 col-md-6">
        <a href="~/Calisan/OnlineDers">
            <div class="card analytic-card card-blue">
                <div class="card-body">
                    <div class="row align-items-center mb-30">
                        <div class="col-auto">
                            <i class="fas fa-book text-blue f-18 analytic-icon"></i>
                        </div>
                        <div class="col text-right">
                            <h3 class="mb-5 text-white"> Teknik Resim </h3>
                            <h6 class="mb-0 text-white">yagmur dapaklı</h6>
                        </div>
                    </div>
                    <div class="progress progress-sm" style="margin-top: 15px;">
                        <div class="progress-bar bg-success" role="progressbar" aria-valuenow="78" aria-valuemin="0" aria-valuemax="100" style="width: 78%;"></div>
                    </div>
                </div>
            </div>
        </a>
    </div>
    <div class="col-xl-4 col-md-6">
        <a href="~/Calisan/SınıfIcıDers">
            <div class="card analytic-card card-yellow">
                <div class="card-body">
                    <div class="row align-items-center mb-30">
                        <div class="col-auto">
                            <i class="fas fa-book text-orange f-18 analytic-icon"></i>
                        </div>
                        <div class="col text-right">
                            <h3 class="mb-5 text-white">Klavye Eğitimi </h3>
                            <h6 class="mb-0 text-white">cemre akanturk</h6>
                        </div>
                    </div>
                    <div class="progress progress-sm" style="margin-top: 15px;">
                        <div class="progress-bar bg-success" role="progressbar" aria-valuenow="78" aria-valuemin="0" aria-valuemax="100" style="width: 78%;"></div>
                    </div>
                </div>

            </div>
        </a>
    </div>


</div>
